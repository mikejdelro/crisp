<!doctype html>
<html lang="en">

<meta property="og:title" content="mikejdelro" />
<meta property="og:type" content="Michael del Rosario's website" />
<meta property="og:url" content="https://michaeldelrosar.io" />
<meta property="og:image" content="https://michaeldelrosar.io/content/images/2019/05/icons-1.png" />
<meta property="og:description" content="It's mikey's website! Hi!" />

	<head>
		{{! Document Settings }}
		<meta http-equiv="Content-Type" content="text/html" charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

		{{! Page Meta }}
		<title>{{meta_title}}</title>
		
		{{! Prism Syntax Highlighting }}
		<link rel="stylesheet" type="text/css" href="{{asset '/prism/prism.css'}}" />
		<script type="text/javascript" src="{{asset '/prism/prism.js'}}"></script>

		{{! Table of Contents Fix }}
		<script type="text/javascript" src="{{asset '/js/toc.js'}}"></script>
		
		{{! kg-gallery support }}
		<script type="text/javascript" src="{{asset '/js/kg-gallery.js'}}"></script>

		{{! Google Fonts }}
		<link href='//fonts.googleapis.com/css?family=Open+Sans:300italic,700italic,300,700' rel='stylesheet' type='text/css'>
		<link href='//fonts.googleapis.com/css?family=Montserrat' rel='stylesheet' type='text/css'>
		
		{{! Stylesheets }}
		<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr"
		crossorigin="anonymous">
		<link rel="stylesheet" type="text/css" href="{{asset '/styles/crisp.css'}}">
		
		{{! Responsive Meta Tags }}
		<meta name="HandheldFriendly" content="True" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
		<!--[if lt IE 9]>
					<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
					<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
				<![endif]-->
		{{{block "head"}}}
		{{ghost_head}}
	</head>

	<body class="{{body_class}}">
		<div align="center">
			<header id="header" {{#if @site.cover_image}} style="background-image: url('{{@site.cover_image}}')" {{/if}}>
			{{#if @site.logo}}<a id="logo" href="{{@site.url}}"><img src="{{@site.logo}}" alt="{{@site.title}}" align="center" /></a>{{/if}}
			<h1><a href="{{@site.url}}">{{@site.title}}</a></h1>
			<p>{{@site.description}}</p>
			{{> follow}}
			{{navigation}}
			<hr>
			<a rel="license" href="http://creativecommons.org/licenses/by-nc-sa/4.0/"><img alt="This work is licensed under a Creative Commons Attribution-NonCommercial-ShareAlike 4.0 International License" style="border-width:0" src="https://i.creativecommons.org/l/by-nc-sa/4.0/80x15.png" /></a>
			</br>
			</header>
		</div>
		<main id="content">
			{{{body}}}
		</main>
		<footer id="footer">
			<section id="footer-message">
				</br>
				These works are licensed under a <a rel="license" href="http://creativecommons.org/licenses/by-nc-sa/4.0/">Creative Commons Attribution-NonCommercial-ShareAlike 4.0 International License</a>.
				</br>
			</section>
			{{ghost_foot}}
		</footer>
	</body>

</html>